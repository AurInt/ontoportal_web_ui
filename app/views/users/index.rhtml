
<table cellpadding="0" cellspacing="0">
  <tr>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Username</th>
	<th>Email</th>
	<th>Roles</th>
	<th>Actions</th>
  </tr>
  
<% for user in @users %>
  <tr class="<%= cycle("odd", "even")%>">
	<td><%=user.firstname%></td>
	<td><%=user.lastname%></td>
	<td><%=user.username%></td>
	<td><%=user.email%></td>
	<td><%=user.roles.join(" | ")%></td>
	<td><%=link_to "View" ,user_path(user.id)%> | Edit | Delete</td>
  </tr>
<% end %>
</table>

