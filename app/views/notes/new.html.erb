<h1>New note</h1>

<% form_for(@note) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :subject %><br />
    <%= f.text_field :subject %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.label :author %><br />
    <%= f.text_field :author %>
  </p>
  <p>
    <%= f.label :archived %><br />
    <%= f.check_box :archived %>
  </p>
  <p>
    <%= f.label :hasStatus %><br />
    <%= f.text_field :hasStatus %>
  </p>
  <p>
    <%= f.label :ontology_id %><br />
    <%= f.text_field :ontology_id %>
  </p>
  <p>
    <%= f.label :concept_id %><br />
    <%= f.text_field :concept_id %>
  </p>
  <p>
    <%= f.label :annotates %><br />
    <%= f.text_field :annotates %>
  </p>
  <p>
    <%= f.label :annotated_by %><br />
    <%= f.text_area :annotated_by %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', notes_path %>