 <table class="annotations" cellpadding=0 cellspacing=0 width="100%">    
<% for annotation in @annotations%>
<%color = cycle("","alt") %>
  <tr class="<%=color%>" >
     <td colspan="2" style="padding:3px;"><a href="<%=annotation.url%>" target="_new"><%=annotation.description%></a></td>
  </tr>	   
  <tr  class="<%=color%>" >
  		<td style="border-top:0px;padding:3px;">ID: <%=annotation.local_id%></td><td  style="border-top:0px;padding:3px;"> Annotation Context: <%=annotation.item_key%></td>
  </tr>
 
 <%end%>
	<tr>
		<td colspan="2" align="right">
			<%if params[:start].to_i > 0%>
			<a href="#" onclick="jQuery('#innerannot<%=params[:index]%>').load('/respage/?ontology=<%=@ontology.id%>&concept=<%=@concept.id%>&resource=<%=params[:resource]%>&start=<%=params[:start].to_i-10%>&end=<%=params[:start]%>&index=<%=params[:index]%>');return false;">&lt;&lt; Previous Page</a>
		<%end%>
		 &nbsp;&nbsp; 
		<%if params[:end].to_i <= params[:total].to_i+10%>
				<a href="#" onclick="jQuery('#innerannot<%=params[:index]%>').load('/respage/?ontology=<%=@ontology.id%>&concept=<%=@concept.id%>&resource=<%=params[:resource]%>&start=<%=params[:end]%>&end=<%=params[:end].to_i+10%>&index=<%=params[:index]%>');return false;">Next Page &gt;&gt;</a>
		<%end%>
	</tr>
</table>